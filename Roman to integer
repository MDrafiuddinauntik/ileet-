


class Solution:
    def romanToInt(self, s: str) -> int:
        dict={"I":1,"V":5,"X":10,"L":50,"C":100,"D":500,"M":1000}
        sum=0
        for i in range(len(s)-1,-1,-1):
            if i<len(s)-1:
             if ((s[i+1]=='X'or s[i+1]=='V') and s[i]=='I') or ((s[i+1]=='L'or s[i+1]=='C') and s[i]=='X') or ((s[i+1]=='M'or s[i+1]=='D') and s[i]=='C'):
                sum-=dict[s[i]]
            else:
                sum+=dict[s[i]]
        return sum
